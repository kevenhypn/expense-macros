{
  "expo": {
    "name": "Expense Macros",
    "slug": "expense-macros",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "expense-macros",
    "userInterfaceStyle": "dark",
    "splash": {
      "backgroundColor": "#121212"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.expense.macros"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#121212"
      },
      "package": "com.expense.macros"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
